insert into reino (nome, descricao) values 
('animalia', 'O Reino Animalia (ou Reino Animal) é composto por organismos pluricelulares, eucariontes e heterotróficos (não produzem seu próprio alimento). As características principais incluem a capacidade de se mover, responder a estímulos do ambiente e respiração aeróbia (que utiliza oxigênio). O reino é diversificado e se divide principalmente em vertebrados (com coluna vertebral) e invertebrados (sem coluna vertebral). ');
